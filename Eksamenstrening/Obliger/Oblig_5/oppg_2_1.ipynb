   "source": [
    "import sympy as sp\n",
    "\n",
    "r, t = sp.symbols('r theta')\n",
    "\n",
    "x = r*sp.cos(t)\n",
    "y = r*sp.sin(t)\n",
    "z = 16 - x**2 - y**2\n",
    "\n",
    "f_rt = z*r\n",
    "display(f_rt)\n",
    "print(f_rt)"
   ]
