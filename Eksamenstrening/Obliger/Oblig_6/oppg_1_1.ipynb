   "source": [
    "import sympy as sp\n",
    "\n",
    "x, y = sp.symbols('x y')\n",
    "\n",
    "z = 5 - x - y\n",
    "\n",
    "T_x = sp.Matrix([\n",
    "    1, 0, z.diff(x)\n",
    "])\n",
    "\n",
    "T_y = sp.Matrix([\n",
    "    0, 1, z.diff(y)\n",
    "])\n",
    "\n",
    "f_xy = T_x.cross(T_y).norm()\n",
    "\n",
    "display(f_xy)\n",
    "print(f_xy)\n",
    "\n",
    "b_1 = sp.solve(z,x)[0]\n",
    "display(b_1)\n",
    "print(b_1)"
   ]
  }
